<!DOCTYPE html>
<html>
  <head>
    <title>Udio</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  	<header>
      <div>
        <%= link_to "Udio", '#', id: "logo" %>
        <nav>
          <ul>
            <li><%= link_to "Home",   '#' %></li>
            <li><%= link_to "Help",   '#' %></li>
            <li><%= link_to "Log in", '#' %></li>
          </ul>
        </nav>
      </div>
    </header>
    <div>
      <%= yield %>
    </div>  
  </body>
</html>
